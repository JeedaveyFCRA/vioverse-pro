{
  "name": "vioverse-v3",
  "version": "3.0.0",
  "private": true,
  "description": "Enterprise-grade credit report violation analysis system",
  "author": "Vioverse Team",
  "license": "PROPRIETARY",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "build:production": "npm install --legacy-peer-deps && npm run ci && next build",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "ci": "npm run typecheck && npm run lint && npm run test",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config=lighthouserc.mobile.json",
    "lighthouse:ci": "npm run build && npm start & npx wait-on http://localhost:3000 --timeout=30000 && npm run lighthouse && kill $(lsof -t -i:3000)",
    "lighthouse:desktop": "lhci autorun --config=lighthouserc.json",
    "perf": "npm run build && npm start & npx wait-on http://localhost:3000 && npm run lighthouse:desktop && npm run lighthouse:mobile && kill $(lsof -t -i:3000)",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "playwright test tests/a11y.spec.ts --project=chromium",
    "test:keyboard": "playwright test tests/keyboard.spec.ts --project=chromium",
    "test:accessibility": "npm run test:a11y && npm run test:keyboard",
    "test:security": "playwright test tests/security.spec.ts --project=chromium",
    "test:responsive": "playwright test tests/responsive.spec.ts --project=chromium",
    "test:mobile": "playwright test tests/mobile.spec.ts",
    "test:visual": "playwright test tests/visual-regression.spec.ts --project=chromium",
    "test:visual:update": "playwright test tests/visual-regression.spec.ts --project=chromium --update-snapshots",
    "test:all": "npm run test && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx scripts/seed.ts",
    "db:studio": "prisma studio",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "analyze": "ANALYZE=true next build"
  },
